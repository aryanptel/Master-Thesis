<?xml version="1.0" ?>
<robot name="simbot" xmlns:xacro="http://www.ros.org/wiki/xacro">

<xacro:include filename="$(find simbot_description)/urdf/materials.xacro" />
<xacro:include filename="$(find simbot_description)/urdf/simbot.trans" />
<xacro:include filename="$(find simbot_description)/urdf/simbot.gazebo" />
<link name="base_link">
  <inertial>
    <origin xyz="0.0038736053620947856 1.5626062597793743e-06 0.05291829347376089" rpy="0 0 0"/>
    <mass value="2.3138752403503746"/>
    <inertia ixx="0.007906" iyy="0.008385" izz="0.01478" ixy="-0.0" iyz="-0.0" ixz="-0.000337"/>
  </inertial>
  <visual>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://simbot_description/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://simbot_description/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="left_wheel_1">
  <inertial>
    <origin xyz="-5.790023100122029e-18 0.012499999999999997 0.0" rpy="0 0 0"/>
    <mass value="0.1826050729899067"/>
    <inertia ixx="0.000124" iyy="0.000228" izz="0.000124" ixy="0.0" iyz="-0.0" ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="-0.0 -0.1 -0.05" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://simbot_description/meshes/left_wheel_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="-0.0 -0.1 -0.05" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://simbot_description/meshes/left_wheel_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="righ_wheel_1">
  <inertial>
    <origin xyz="-5.790023100122029e-18 -0.012499999999999997 0.0" rpy="0 0 0"/>
    <mass value="0.1826050729899067"/>
    <inertia ixx="0.000124" iyy="0.000228" izz="0.000124" ixy="-0.0" iyz="0.0" ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="-0.0 0.1 -0.05" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://simbot_description/meshes/righ_wheel_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="-0.0 0.1 -0.05" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://simbot_description/meshes/righ_wheel_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="lidar_1">
  <inertial>
    <origin xyz="9.228728892196614e-16 -1.0275503958549896e-15 0.012499999999998387" rpy="0 0 0"/>
    <mass value="0.10789485809488787"/>
    <inertia ixx="4.1e-05" iyy="4.1e-05" izz="7e-05" ixy="0.0" iyz="0.0" ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="-0.05 0.0 -0.11" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://simbot_description/meshes/lidar_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="-0.05 0.0 -0.11" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://simbot_description/meshes/lidar_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<joint name="Revolute 1" type="continuous">
  <origin xyz="0.0 0.1 0.05" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="left_wheel_1"/>
  <axis xyz="0.0 1.0 0.0"/>
</joint>

<joint name="Revolute 2" type="continuous">
  <origin xyz="0.0 -0.1 0.05" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="righ_wheel_1"/>
  <axis xyz="0.0 -1.0 0.0"/>
</joint>

<joint name="Revolute 3" type="continuous">
  <origin xyz="0.05 0.0 0.11" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="lidar_1"/>
  <axis xyz="0.0 -0.0 1.0"/>
</joint>

</robot>
